{
  "name": "yashvi-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-three/drei": "^9.114.5",
    "@react-three/fiber": "^8.15.16",
    "@react-three/postprocessing": "^2.15.12",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "next": "14.2.33",
    "postprocessing": "^6.35.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.159.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.5",
    "@types/three": "^0.180.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,scss}": [
      "prettier --write"
    ]
  }
}
