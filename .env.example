# Example environment variables for local development and Vercel configuration.
# Copy this file to `.env.local` and update the values before running locally.

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/realestate_db"

# Authentication
NEXTAUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# Cloudinary
CLOUDINARY_CLOUD_NAME="your-cloudinary-cloud-name"
CLOUDINARY_API_KEY="your-cloudinary-api-key"
CLOUDINARY_API_SECRET="your-cloudinary-api-secret"

# Analytics
NEXT_PUBLIC_ANALYTICS_WRITE_KEY=pk_prod_replace_me
NEXT_PUBLIC_ANALYTICS_DATASET=primary

# Backend API surface consumed by the app and health check.
API_BASE_URL=https://api.example.com

# Optional shared secret forwarded by uptime monitoring probes.
HEALTHCHECK_TOKEN=
